#include <iostream>


// if i modulo 3 print "Fizz", if i modulo 5 print 'Buzz', if i modulo 3 and 5 print "fizzbuzz"
int main(){

for(int i = 0; i <= 100; i++){
    std::cout << i << "\n";
    if (i % 3 == 0){
        std::cout << "Fizz\n";
    }

    else if (i % 5 == 0 ){
        std::cout << "Buzz\n";

    }


    else if (i % 5  && i % 3);{
        std::cout << "FizzBuzz\n";
    }

}
}
